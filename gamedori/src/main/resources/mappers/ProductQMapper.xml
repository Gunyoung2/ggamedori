<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="game.dori.mapper.productQMapper">

	
	<!-- 마이페이지 -상품문의 리스트 출력 -->
	<select id="selectList" resultType="productqq_VO" parameterType="int">
		SELECT *
			FROM product_q_tb q
			INNER JOIN product_tb p
			 on q.product_tb_idx = p.prod_idx
			WHERE member_tb_idx = #{member_idx}
		
	</select>
		<!-- int 타입의 매개변수 member_idx 값을 받아서 칼럼 전체의 값을 p 객체로 형태로 반환 -->
	
</mapper>